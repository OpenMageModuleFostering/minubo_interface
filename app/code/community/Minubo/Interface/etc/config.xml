	</global>
	<frontend>
		<routers>
			<magento_interface>
				<use>standard</use>
				<args>
					<module>Minubo_Interface</module>
					<frontName>minuboface</frontName>
				</args>
			</magento_interface>
		</routers>
		<layout>
			<updates>
				<interface>
					<file>interface.xml</file>
				</interface>
			</updates>
		</layout>
	</frontend>
