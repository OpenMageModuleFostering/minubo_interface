					</creditmemos>
					<creditmemoitems>
						<table>sales_flat_creditmemo_item</table>
					</creditmemoitems>
					<invoices>
						<table>sales_flat_invoice</table>
					</invoices>
					<invoiceitems>
						<table>sales_flat_invoice_item</table>
					</invoiceitems>
				</entities>
			</minubo_mysql4>
		</models>
		<helpers>
			<minubo_interface>
				<class>Minubo_Interface_Helper</class>
			</minubo_interface>
		</helpers>
		<blocks>
			<minubo_interface>
				<class>Minubo_Interface_Block</class>
			</minubo_interface>
		</blocks>
