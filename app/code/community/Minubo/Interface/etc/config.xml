<?xml version="1.0" encoding="UTF-8"?>
<config>
	<modules>
		<Minubo_Interface>
			<version>1.0.10</version>
		</Minubo_Interface>
	</modules>
	<global>
		<models>
			<minubo_interface>
				<class>Minubo_Interface_Model</class>
				<resourceModel>minubo_mysql4</resourceModel>
			</minubo_interface>
			<minubo_mysql4>
				<class>Minubo_Interface_Model_Mysql4</class>
				<entities>
					<orders>
						<table>sales_flat_order</table>
					</orders>
					<sales_flat_order_address>
						<table>sales_flat_order_address</table>
					</sales_flat_order_address>
                    <sales_flat_order_payment>
                        <table>sales_flat_order_payment</table>
                    </sales_flat_order_payment>
                    <orderitems>
						<table>sales_flat_order_item</table>
